
    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <title></title>
    </head>
    <body>
        <%- include('../partials/userheader.ejs') %>
         <!-- Begin Page Content -->
    <div class="container-fluid">

        <!-- Page Heading -->
        
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">All Files</h6>              
                    <button class="btn btn-info  float-end" ><a href="/user/deshboard"> Back </a></button>           
            </div>
            <% if (result.mess.length > 0) { %>
                <div class="alert alert-success mt-2">           
                    <%=result.mess%>                
                </div>                
            <% } %>
            <div class="card-body">
                <div class="table-responsive ">
                    <table class="table table-bordered" id="dataTable" width="100%" >
                        <thead >
                            <tr>
                                <th>Sr</th>
                                <th>file1</th>
                                <th>file2</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>Sr</th>
                                <th>file1</th>
                                <th>file2</th>                                                                      
                            </tr>
                        </tfoot>
                        <tbody>                            
                            <% result.multiplefile.forEach((resfile,index)=>{%>
                                <tr>
                                    <td>
                                        <%=index+1%>
                                    </td>
                                    <td>                                        
                                        <img src="/<%=resfile.photo%>" width="100" />
                                    </td>
                                    <td> 
                                        <% resfile.zipfile.forEach((file,key)=>{%>                                       
                                            <a href="/<%=file%>">File <%=key+1%></a><br>
                                        <%})%>
                                    </td> 
                                </tr>
                                <%})%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>        
    </body>
    </html>
    <%- include('../partials/footer.ejs') %>
    
   
